<!DOCTYPE html>
<html>
<head>
  <title>The Picture Closet</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="../../../../../Downloads/6_navigation_2/style.css" rel="stylesheet" type="text/css" />
  <link rel="stylesheet" type="text/css" href="http://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.1/normalize.min.css">
</head>
<body>

<header>
  <div id="header_container">
    <div id="header_logo">
      <%= link_to image_tag("NewPictureClosetLogo.png"), root_path %>
    </div><!--end header_logo-->
  </div><!--end header-->
</header>

<nav>
  <div id="nav_container">

    <ul id="menu">
      <li><a href="#">Home</a></li>
      <li><a href="#">About</a></li>
      <li><a href="#">Portfolio</a></li>
      <li><a href="#">Relatives</a></li>
      <li><a href="#">Contact</a></li>
    </ul>
  </div><!--end navigation-->
</nav>

<div id="content">
  <div id="content_container">
    <%= yield %>
    
    <!--  1/3 x 2/3 -->
    <div class="one_third">
      1/3 Column
    </div>
    <div class="two_third">
      2/3 Column
    </div>
    <div class="resetColumnSet"></div>

    <!--  1/3 x 1/3 x 1/3 -->
    <div class="one_third">
      1/3 Column
    </div>
    <div class="one_third">
      1/3 Column
    </div>
    <div class="one_third">
      1/3 Column
    </div>
    <div class="resetColumnSet"></div>

    <!--  1/4 x 1/4 x 1/4 x 1/4 -->
    <div class="one_fourth">1/4 Column</div>
    <div class="one_fourth">1/4 Column</div>
    <div class="one_fourth">1/4 Column</div>
    <div class="one_fourth">1/4 Column</div>
    <div class="resetColumnSet"></div>

  </div><!--end content_container-->
</div><!--end content-->

<footer>
  <div id="footer_container">

    <!--  2/3 x 1/3 -->
    <div class="two_third">
      1/3 Column
    </div>
    <div class="one_third">
      2/3 Column
    </div>
    <div class="resetColumnSet"></div>

  </div><!--end footer_container-->
</footer>

</body>
</html>
